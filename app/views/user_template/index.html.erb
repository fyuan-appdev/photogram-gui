<h1>List of users</h1>

<table border="1">
  <tr>
    <th>
      ID
    </th>
    <th>
      Username
    </th>
    <th></th>
  </tr>


  <% @list_of_users.each do |a_user| %>
  <tr>
    <th>
      <%= a_user.id%>
    </th>
    <th>
      <%= a_user.username%>
    </th>
    <th>
      <a href="/movies/<%= a_user.username %>">
        Show details
    </th>
  </tr>
  <% end %>
</table>
